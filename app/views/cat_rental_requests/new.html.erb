<h1> Request form! </h1>
<form action="<%= cat_cat_rental_requests_url %>" method="POST">
  <input type="hidden" name= "cat_rental_request[cat_id]" value=<%=params[:cat_id]%>
  
  <label for="start_date">Start date</label>
  <input name="cat_rental_request[start_date]" id="start_date" type='date'>
  <br>

  <label for="end_date">End date</label>
  <input name="cat_rental_request[end_date]" id="end_date" type='date'>
  <br>
  <br>

  <input type="submit" value="Submit request">
  <br>
  <br>
</form>

<%= link_to "Back", :back %>
